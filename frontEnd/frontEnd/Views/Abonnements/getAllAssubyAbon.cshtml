@model IEnumerable<Model.Abonnement.Assurence>

@{
    ViewBag.Title = "getAllAssubyAbon";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>getAllAssubyAbon</title>
</head>
<body>
   
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.nomAssu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.descAssu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dureAss)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dateDeburAssu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dateFintAssu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.prixAss)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TypeAssurence)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nomAssu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.descAssu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dureAss)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dateDeburAssu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dateFintAssu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.prixAss)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TypeAssurence)
            </td>

        </tr>
    }
    
    </table>
</body>
</html>
